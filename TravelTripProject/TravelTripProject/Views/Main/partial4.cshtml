@using TravelTripProject.Models.classes
@model List<Blog>


<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogFoto" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,75)...</p>
                <br />
                
                <p><a href="/Blog/BlogDetay/@x.ID"><span class="read">Devamını Oku >></span></a></p>
                <br />
            </div>
           
            <div class="clear"></div>

        }
    </div>
    @Html.Action("partial5","Main")
</div>

